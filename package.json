{
  "name": "itodo",
  "displayName": "iTodo",
  "description": "",
  "version": "0.0.2",
  "publisher": "waifuproject",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "itodo.bartext": {
          "type": "string",
          "default": "itodo",
          "description": "json bin secure key"
        },
        "itodo.jsonbinSecretKey": {
          "type": "string",
          "default": false,
          "description": "json bin secure key"
        },
        "itodo.jsonbinID": {
          "type": "string",
          "default": false,
          "description": "json bin data id"
        }
      }
    },
    "commands": [
      {
        "command": "itodo.syncDataToLocal",
        "title": "[itodo] 获取远程数据"
      },
      {
        "command": "itodo.syncDataToRemote",
        "title": "[itodo] 同步数据到远程"
      },
      {
        "command": "itodo.showTodoUI",
        "title": "[itodo] 显示任务界面"
      },
      {
        "command": "itodo.showTotalTaskCount",
        "title": "[itodo] 显示当前有多少任务"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
